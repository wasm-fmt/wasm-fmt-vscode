{
	"name": "wasm-fmt",
	"displayName": "wasm fmt",
	"description": "Code Formatter powered by WebAssembly",
	"author": "magic-akari",
	"publisher": "wasm-fmt",
	"version": "0.6.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/wasm-fmt/wasm-fmt-vscode.git"
	},
	"engines": {
		"vscode": "^1.88.0"
	},
	"packageManager": "pnpm@10.30.2",
	"categories": [
		"Formatters"
	],
	"icon": "images/icon.png",
	"sponsor": {
		"url": "https://github.com/sponsors/magic-akari"
	},
	"activationEvents": [
		"onStartupFinished",
		"onLanguage:astro",
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:csharp",
		"onLanguage:css",
		"onLanguage:cuda-cpp",
		"onLanguage:cuda",
		"onLanguage:dart",
		"onLanguage:dockerfile",
		"onLanguage:dotenv",
		"onLanguage:github-actions",
		"onLanguage:glsl",
		"onLanguage:go",
		"onLanguage:graphql",
		"onLanguage:hlsl",
		"onLanguage:html",
		"onLanguage:ignore",
		"onLanguage:java",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onLanguage:less",
		"onLanguage:lua",
		"onLanguage:markdown",
		"onLanguage:metal",
		"onLanguage:objective-c",
		"onLanguage:objective-cpp",
		"onLanguage:php",
		"onLanguage:properties",
		"onLanguage:proto",
		"onLanguage:proto3",
		"onLanguage:python",
		"onLanguage:sass",
		"onLanguage:scss",
		"onLanguage:shellscript",
		"onLanguage:sql",
		"onLanguage:svelte",
		"onLanguage:systemverilog",
		"onLanguage:textproto",
		"onLanguage:toml",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:verilog",
		"onLanguage:vue",
		"onLanguage:yaml",
		"onLanguage:zig"
	],
	"browser": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "./esbuild.mjs",
		"compile": "./esbuild.mjs",
		"watch": "./esbuild.mjs --watch",
		"version": "node scripts/update-changelog.mjs && git add CHANGELOG.md"
	},
	"devDependencies": {
		"@types/node": "^22.10.1",
		"@types/vscode": "^1.88.0",
		"esbuild": "^0.27.3"
	},
	"dependencies": {
		"@wasm-fmt/clang-format": "^21.1.8",
		"@wasm-fmt/dart_fmt": "^0.4.0",
		"@wasm-fmt/gofmt": "^0.7.2",
		"@wasm-fmt/lua_fmt": "^0.3.2",
		"@wasm-fmt/mago_fmt": "0.6.4",
		"@wasm-fmt/markdown": "0.1.5",
		"@wasm-fmt/ruff_fmt": "^0.15.0",
		"@wasm-fmt/shfmt": "0.2.5",
		"@wasm-fmt/sql_fmt": "^0.2.1",
		"@wasm-fmt/taplo_fmt": "0.2.5",
		"@wasm-fmt/web_fmt": "^0.2.3",
		"@wasm-fmt/yamlfmt": "^0.2.1",
		"@wasm-fmt/zig_fmt": "^0.15.2"
	},
	"keywords": [
		"prettier",
		"formatter",
		"web",
		"wasm",
		"browser"
	]
}
